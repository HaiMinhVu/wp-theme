{
  "name": "upstatement/timber-starter-theme",
  "description": "Starter theme to build a Timber theme",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
    {
      "email": "jared@upstatement.com",
      "name": "jarednova"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
  {
    "type": "package",
    "package": {
      "name": "sellmark/utilities",
      "version": "0.0.1",
      "type": "package",
      "source": {
        "url": "git@git.slmk.dev:Sellmark/SellmarkUtilities.git",
        "type": "git",
        "reference": "master"
      }
    }
  }

  ],
  "require": {
	  "timber/timber": "1.*",
      "sellmark/utilities": "0.0.1",
    "htmlburger/carbon-fields-plugin": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.16|6.*"
  },
  "autoload": {
      "psr-4": {
          "Sellmark\\": "vendor/sellmark/utilities",
          "Pulsar\\": "classes"
      }
  }

}
