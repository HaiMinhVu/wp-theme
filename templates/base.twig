{% block html_head_container %}

{% include 'html-header.twig' %}
{% block head %}
{% endblock %}
</head>
{% endblock %}
<body class="{{body_class}}{{ (breadcrumbs is defined) ? ' has-breadcrumbs' : '' }}" data-template="base.twig">
	<header class="fixed-top" id="myHeader">
		<div class="header-container">
			<nav class="navbar navbar-expand-lg">
				<ul class="navbar-nav">
					<li class="nav-item dropdown ng-tns-c2-0 ng-star-inserted">
						<a class="navbar-brand" routerlink="/" href="/">
							<img class="img-fluid" alt="" src="{{ slmk_site_logo }}">
						</a>
					</li>
					{% for item in nav_items %}
					<li class="nav-item dropdown ng-tns-c2-0 ng-star-inserted">
						{% if item.children|length > 0 %}
						<a aria-expanded="false" aria-haspopup="true" class="nav-link dropdown-toggle pro ng-tns-c2-0 ng-star-inserted" data-toggle="dropdown" href="{{ item.link }}" id="navbarDropdownMenuLink">
							{{ item.name }} <i class="fa fa-angle-down ml-3"></i>
						</a>
						<ul aria-labelledby="navbarDropdownMenuLink" class="dropdown-menu ng-tns-c2-0 ng-star-inserted">
							{% for child in item.children %}
							<li class="nav-item dropdown ng-tns-c2-0 ng-star-inserted">
								<a class="dropdown-item" href="{{ child.link }}">{{ child.name }}</a>
							</li>
							{% endfor %}
						</ul>
						{% else %}
						<a class="nav-link ng-tns-c2-0 ng-star-inserted" href="{{ item.link }}"> {{ item.name }} </a>
						{% endif %}
					</li>
					{% endfor %}
					<li class="nav-item-icons d-lg-flex" style="width:80px">
						<a class="nav-link search search-toggle" aria-controls="collapseExample" href="javascript:void(0)" aria-expanded="false">
							<i class="fa fa-search">
							</i>
						</a>
						<a class="nav-link cart" target="_blank" href="http://www.sellmarknexus.com/checkout/cart.ssp?sc=3&amp;ck=RLi-rpzgAVWwmaRg&amp;vid=RLi-ruHgAQOKmrLj&amp;cktime=131820&amp;cart=5623&amp;ext=T">
							<i class="fa fa-shopping-cart">
							</i>
							<span class="badge badge-secondary" id="shopping-counter" style="display: none"></span>
							<iframe id="shopping-counter-content" src="/?show_shopping_counter=true" style="border: 0;width: 1px;height: 1px;pointer-events: none; position: fixed;top:0;left:0;"></iframe>
						</a>
						<button id="hamburger-menu" class="navbar-toggler nav-link" type="button">
							<span class="navbar-toggler-icon">
								<i class="fa fa-bars" aria-hidden="true"></i>
								<i class="fa fa-times" aria-hidden="true"></i>
							</span>
						</button>
					</li>
				</ul>
			</nav>
		</div>
		<div class="search-wrapper collapse" id="search-bar" >
			<form method="GET" action="/" class="container" style="position: relative;">
				<div class="row justify-content-between">
					<div class="col">
						<div class="search-box">
							<input class="form-control" name="s" autocomplete="off" placeholder="Search..." type="search">
							<i class="fa fa-search">
							</i>
						</div>
					</div>
					<div class="col-auto align-self-center">
						<span class="close-search search-toggle">
							<i class="fa fa-close">
							</i>
						</span>
					</div>
				</div>
			</form>
		</div>
		{% if breadcrumbs is defined and breadcrumbs|length > 0 %}
		<div class="theme-bread ng-star-inserted nav-breadcrumbs">
			<div class="container">
				<div class="row">
					<div class="col-12">
						<nav aria-label="breadcrumb">
							<ol class="breadcrumb bg-transparent">
								{% for breadcrumb in breadcrumbs %}
								<li class="breadcrumb-item">
									<a href="/category/{{ breadcrumb.id }}">{{ breadcrumb.label }}</a>
								</li>
								{% endfor %}
							</ol>
						</nav>
					</div>
				</div>
			</div>
		</div>
		{% endif %}
		{% include 'partial/overlay-menu.twig' %}
		<div id="search-bar-old" style="display:none">
			{% include 'partial/search.twig' %}
		</div>
	</header>
	<section id="content" role="main" class="content-wrapper">
		<div class="wrapper {{sidebar_class}}">
			{% block content %}
			Sorry, no content
			{% endblock %}
		</div>
	</section>

	{% block footer %}
	{% include 'footer.twig' %}

	{{ function('wp_footer') }}
	<div id="alert-bottom" class="alert alert-success alert-dismissible fade show" role="alert">
		<strong id="alert-bottom-text1" class="alert-bottom-text"></strong> <span id="alert-bottom-text2" class="alert-bottom-text"></span>
		<button type="button" class="close" data-dismiss="alert" aria-label="Close">
			<span aria-hidden="true">&times;</span>
		</button>
	</div>
{% endblock %}


{% block scripts %}
{% endblock %}
</body>
</html>
